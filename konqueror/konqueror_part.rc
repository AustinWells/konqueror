  <Action name="saveremoveviewprofile"/>
  <Action name="loadviewprofile"/>
 </Menu>
 <Separator/>
 <Menu name="Help">
  <Action name="about"/>
 </Menu>
</MenuBar>
<ToolBars>
 <ToolBar fullWidth="true" name="mainToolBar">
  <Action name="up"/>
  <Action name="back"/>
  <Action name="forward"/>
  <Action name="home"/>
  <Separator/>
  <Action name="reload"/>
  <Separator/>
  <Action name="cut"/>
  <Action name="copy"/>
  <Action name="paste"/>
  <Action name="print"/>
  <Separator/>
  <Action name="stop"/>
  <Separator/>
  <Action name="animated_logo"/>
 </ToolBar>
 <ToolBar fullWidth="true" name="locationToolBar">
  <Action name="toolbar_url_combo"/>
 </ToolBar>
</ToolBars>
</QConfig>

